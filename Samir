var c = document.getElementById("myCanvas")
var ctx = c.getContext("2d")

//filled circle main
ctx.beginPath()
ctx.fillStyle = "yellow"
ctx.arc(150, 150, 100, 0, 2 * Math.PI)
ctx.fill()

//outlined circle main
ctx.beginPath()
ctx.strokeStyle = "black";
ctx.arc(150, 150, 100, 0, 2 * Math.PI)
ctx.stroke()

//filled circle left-eye
ctx.beginPath()
ctx.fillStyle = "white"
ctx.arc(110, 110, 15, 0, 2 * Math.PI)
ctx.fill()

//outlined circle left-eye
ctx.beginPath()
ctx.strokeStyle = "black"
ctx.arc(110, 110, 15, 0, 2 * Math.PI)
ctx.stroke()

//filled circle right-eye
ctx.beginPath()
ctx.fillStyle = "white"
